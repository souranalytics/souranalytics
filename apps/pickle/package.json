{
  "name": "pickle",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .next",
    "build": "next build",
    "db:dev": "env-cmd -f .env.local prisma migrate dev",
    "db:generate": "env-cmd -f .env.local prisma generate",
    "db:migrate": "env-cmd -f .env.production prisma migrate deploy",
    "db:push": "env-cmd -f .env.local prisma db push",
    "db:seed": "env-cmd -f .env.local prisma db seed",
    "dev": "next dev --port 3001",
    "generate": "prisma generate",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "next": "12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "shared": "*"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "18.0.5",
    "config": "*",
    "env-cmd": "^10.1.0",
    "eslint": "8.13.0",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^9.0.0",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "tsconfig": "*",
    "typescript": "^4.6.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
