{
  "environments": [
    {
      "id": "sour-analytics",
      "name": "Sour Analytics",
      "region": "eu-west-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "sour-analytics",
          "name": "Sour Analytics",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "cd apps/sour && yarn generate && yarn build",
          "startCommand": "cd apps/sour && yarn start",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com",
            "DATABASE_URL": {
              "fromParameterStore": "fc.souranalytics.env.sour-analytics.207714lx.SOUR_DATABASE_URL"
            }
          }
        },
        {
          "id": "pickle",
          "name": "Pickle",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "pickle.souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "cd apps/pickle && yarn generate && yarn build",
          "startCommand": "cd apps/pickle && yarn start",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com",
            "DATABASE_URL": {
              "fromParameterStore": "fc.souranalytics.env.sour-analytics.x47614fl.PICKLE_DATABASE_URL"
            }
          }
        },
        {
          "id": "gherkin",
          "name": "Gherkin",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "gherkin.souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "cd apps/gherkin && yarn generate && yarn build",
          "startCommand": "cd apps/gherkin && yarn start",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com",
            "DATABASE_URL": {
              "fromParameterStore": "fc.souranalytics.env.sour-analytics.dc7514ia.GHERKIN_DATABASE_URL"
            }
          }
        }
      ]
    }
  ]
}
