{
  "environments": [
    {
      "id": "sour-analytics",
      "name": "Sour Analytics",
      "region": "eu-west-1",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "sour-analytics",
          "name": "Sour Analytics",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "yarn turbo run build db:generate --scope=sour-analytics",
          "startCommand": "yarn turbo run start --scope=sour-analytics",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com"
          }
        },
        {
          "id": "pickle",
          "name": "Pickle",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "pickle.souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "yarn turbo run build db:generate --scope=pickle",
          "startCommand": "yarn turbo run start --scope=pickle",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com"
          }
        },
        {
          "id": "gherkin",
          "name": "Gherkin",
          "type": "fargate",
          "cpu": 0.25,
          "memory": 0.5,
          "domain": "gherkin.souranalytics.com",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "yarn turbo run build db:generate --scope=gherkin",
          "startCommand": "yarn turbo run start --scope=gherkin",
          "envVariables": {
            "NEXT_PUBLIC_URL_GHERKIN": "https://gherkin.souranalytics.com",
            "NEXT_PUBLIC_URL_PICKLE": "https://pickle.souranalytics.com",
            "NEXT_PUBLIC_URL_SOUR": "https://souranalytics.com"
          }
        }
      ]
    }
  ]
}
